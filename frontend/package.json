{
    "name": "shopping-cart",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prettier:test": "prettier --check \"src/**/*.ts*\"",
        "prettier:fix": "prettier --write \"src/**/*.ts*\"",
        "tsc": "tsc --noEmit",
        "lint": "npm run prettier:test && npm run tsc && npm run lint:test",
        "lint:test": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prepare": "husky install"
    },
    "dependencies": {
        "@paypal/react-paypal-js": "^8.1.3",
        "clsx": "^2.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-redux": "8.1.3",
        "react-router-dom": "^6.18.0",
        "redux": "4.2.1",
        "redux-devtools-extension": "2.13.9",
        "redux-thunk": "2.4.2",
        "sass": "^1.69.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/react": "18.2.15",
        "@types/react-dom": "18.2.7",
        "@types/react-redux": "7.1.30",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "6.0.0",
        "@typescript-eslint/parser": "6.0.0",
        "@vitejs/plugin-react": "4.0.3",
        "eslint": "8.45.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "0.4.3",
        "husky": "^8.0.3",
        "prettier": "3.0.3",
        "typescript": "5.0.2",
        "vite": "4.4.5"
    }
}
